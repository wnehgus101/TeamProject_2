<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ExtraText">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        tools:ignore="SpeakableTextPresentCheck,UselessParent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:ignore="ScrollViewSize">

            <View
                android:id="@+id/view4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#1B1818" />

            <Button
                android:id="@+id/backButton"
                android:layout_width="60dp"
                android:layout_height="50dp"
                android:background="@android:drawable/ic_menu_revert"
                tools:ignore="SpeakableTextPresentCheck" />

            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#1B1818" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="144dp"
                android:layout_marginStart="0dp"
                android:layout_marginEnd="0dp">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="157dp"
                    android:layout_marginTop="21dp"
                    android:text="000 수의사"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="159dp"
                    android:layout_marginTop="55dp"
                    android:text="경력: 00000"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="159dp"
                    android:layout_marginTop="97dp"
                    android:text="위치: 00000"
                    tools:ignore="HardcodedText" />

                <Button
                    android:id="@+id/button2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="80dp"
                    android:layout_marginEnd="48dp"
                    android:background="#2196F3"
                    android:text="예약하기"
                    tools:ignore="HardcodedText,TextContrastCheck" />

                <Button
                    android:id="@+id/button3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="19dp"
                    android:layout_marginEnd="48dp"
                    android:background="#2196F3"
                    android:text="상담 불가"
                    tools:ignore="HardcodedText,TextContrastCheck,TextContrastCheck" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="109dp"
                    android:layout_height="101dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="23dp"
                    app:srcCompat="@drawable/boy"
                    tools:ignore="ContentDescription" />

            </RelativeLayout>

            <View
                android:id="@+id/view2"
                android:layout_width="419dp"
                android:layout_height="1dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="0dp"
                android:background="#1B1818" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="68dp">

                <Button
                    android:id="@+id/프로필"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="29dp"
                    android:layout_marginBottom="11dp"
                    android:background="#2196F3"
                    android:text="프로필"
                    tools:ignore="HardcodedText,TextContrastCheck" />

                <Button
                    android:id="@+id/경력"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="152dp"
                    android:layout_marginTop="11dp"
                    android:background="#2196F3"
                    android:text="경력"
                    tools:ignore="HardcodedText,TextContrastCheck" />

                <Button
                    android:id="@+id/상담후기"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="9dp"
                    android:layout_marginEnd="41dp"
                    android:background="#2196F3"
                    android:text="상담 후기"
                    tools:ignore="HardcodedText,TextContrastCheck" />
            </RelativeLayout>

            <View
                android:id="@+id/view5"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_weight="1"
                android:background="#1B1818"
                tools:ignore="InefficientWeight" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="465dp">

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="381dp"
                    android:layout_height="44dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="32dp"
                    android:layout_marginEnd="17dp"
                    android:text="한줄소개: 0000000000"
                    android:textSize="17sp"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="381dp"
                    android:layout_height="44dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="16dp"
                    android:layout_marginBottom="49dp"
                    android:text="위치: 0000000000"
                    android:textSize="17sp"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="381dp"
                    android:layout_height="44dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="15dp"
                    android:layout_marginBottom="109dp"
                    android:text="상담 가능 시간 및 요일: 0000000000"
                    android:textSize="17sp"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="381dp"
                    android:layout_height="44dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="16dp"
                    android:layout_marginBottom="221dp"
                    android:text="치료 가능 동물: 0000000000"
                    android:textSize="17sp"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="381dp"
                    android:layout_height="44dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="15dp"
                    android:layout_marginBottom="167dp"
                    android:text="상담료: 0000000000"
                    android:textSize="17sp"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="381dp"
                    android:layout_height="44dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="144dp"
                    android:text="성격: 0000000000"
                    android:textSize="17sp"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="381dp"
                    android:layout_height="44dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="88dp"
                    android:text="경력: 0000000000"
                    android:textSize="17sp"
                    tools:ignore="HardcodedText" />

            </RelativeLayout>

        </LinearLayout>
    </ScrollView>

</RelativeLayout>